
#include <Servo.h>

Servo laserServo;
Servo baseServo;

int tonePin = 8;
int startButton = 4;
int rightButton = 3;
int leftButton = 2;
int rightState = 1;
int startState = 1;
int leftState = 1;
int startx = 10;
int endx = 130;
int y = 45;
int offset = 10;
int  timelimit = 60;

void setup() {
  Serial.begin(9600);
  pinMode(leftButton, INPUT_PULLUP);
  pinMode(rightButton, INPUT_PULLUP);
  pinMode(startButton, INPUT_PULLUP);
  laserServo.attach(11);
  baseServo.attach(10);
  
  laserServo.write(y);
  baseServo.write(0);
  delay(30);
}


void loop() {
  rightState = digitalRead(rightButton);
  startState = digitalRead(startButton);
  leftState = digitalRead(leftButton);
  Serial.println(rightState);
  Serial.println(startState);
  Serial.println(leftState);
  delay(200);
  if(rightState == LOW){
    tone(tonePin, 116, 42.0572916667);
    y = y-offset;
    laserServo.write(y);              // tell servo to go to position in variable 'pos'
    delay(15);
    
    
  }else if(startState == LOW){
   tone(tonePin, 116, 42.0572916667);
  sweep(baseServo,startx,endx);
    
  }else if(leftState == LOW){
    tone(tonePin, 116, 42.0572916667);
    y = y+offset;
    laserServo.write(y);              // tell servo to go to position in variable 'pos'
    delay(15);
    
  }
  
   
}

void sweep(Servo myservo,int startValue,int stopValue){

int timel = timelimit;
  while(timel > 0){
    for (int pos = startValue; pos <= stopValue; pos += 1) { // goes from 0 degrees to 180 degrees
    // in steps of 1 degree
    myservo.write(pos);              // tell servo to go to position in variable 'pos'
    timel = timel -.075;
    delay(15);                       // waits 15ms for the servo to reach the position
  }
  for (int pos = stopValue; pos >= startValue; pos -= 1) { 
    myservo.write(pos);
    timel = timel -.075;             
    delay(15);                      
  }
  }
  midi3();
}
